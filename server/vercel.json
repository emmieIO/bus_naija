{
    "installCommand":"npm i --legacy-peer-deps",
    "version":"2",
    "builds": [
        {
        "src": "index.js",
        "use": "@vercel/node"
        },
        {
        "src": "src/**/*",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/"
        }
    ]
}