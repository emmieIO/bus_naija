{
    "installCommand":"npm i --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
        "src": "index.js",
        "use": "@vercel/node"
        },
        {
        "src": "api/**/*",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/api/.*",
        "dest": "api/index.js"
        }
    ]
}